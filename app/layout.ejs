<!DOCTYPE html>
<html lang="en">

  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title><%- title %></title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.13.0/css/all.min.css" />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Lato:wght@400;700&family=Roboto+Mono&display=swap"
      rel="stylesheet">
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
    <link rel="stylesheet" href="./assets/style/all.css">
  </head>

  <body>
    <div class="d-flex min-vh-100">
      <!-- nav-side -->
      <nav class="bg-light min-vh-100">
        <section class="py-8 nav-side d-flex flex-column h-100">
          <!-- logo -->
          <h1 class="mb-8 px-8">
            <a href="index.html">
              <img src="./assets/images/logo-ANSWER.svg" alt="logo">
            </a>
          </h1>
          <!-- list -->
          <ul class="fw-bold list-unstyled">
            <li class="mb-3 px-8">
              <a class="nav-link link-dark p-1" href="#">
                <span class="material-icons pe-4 align-middle">assessment</span>
                Dashboard
              </a>
            </li>
            <li class="mb-3 px-8">
              <a class="nav-link link-dark p-1" href="#">
                <span class="material-icons pe-4  align-middle">question_answer</span>
                Question
              </a>
            </li>
            <li class="mb-3 px-8">
              <a class="nav-link link-dark p-1" href="#">
                <span class="material-icons pe-4 align-middle">assignment</span>
                Assignment
              </a>
            </li>
            <li class="mb-3 px-8">
              <a class="nav-link link-dark p-1" href="#">
                <span class="material-icons pe-4 align-middle">video_library</span>
                Course
              </a>
            </li>
            <li class="mb-3 px-8">
              <a class="nav-link link-dark p-1 active" href="#">
                <span class="material-icons pe-4 align-middle">supervisor_account</span>
                Admin
              </a>
            </li>
            <li class="mb-3 px-8">
              <a class="nav-link link-dark p-1" href="#">
                <span class="material-icons pe-4 align-middle">settings</span>
                Settings
              </a>
            </li>
          </ul>
          <div class="d-flex user-data px-8 mt-auto">
            <img class="rounded-circle wh-40 me-4" src="./assets/images/andychen.jpeg" alt="andy">
            <div>
              <h4 class="fw-bold text-gray-900 mb-1">Andy Chen</h4>
              <p class="fs-5 text-gray-600 mb-0">andy0401@mail.com</p>
            </div>
          </div>
        </section>
      </nav>
      <%- contents %>
    </div>
    <script src="./assets/js/vendors.js"></script>
    <script src="./assets/js/all.js"></script>
  </body>

</html>